{
  "name": "gsgenerator",
  "displayName": "GSgenerator",
  "description": "LEAVE BLANK",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateGetterSetterES6",
        "title": "GSGenerateES6"
      },
      {
        "command": "extension.generateGetterSetterForAllES6",
        "title": "GSGenerateAllES6"
      }
    ],
    "keybindings": [
        {
          "command": "extension.generateGetterSetterES6",
          "key": "ctrl+win+shift+s",
          "mac": "ctrl+f10",
          "when": "editorTextFocus"
        },
        {
          "command": "extension.generateGetterSetterForAllES6",
          "key": "ctrl+win+shift+d",
          "when": "editorTextFocus"
        },
        {
          "key": "ctrl+alt+g", 
          "command": "extension.SelectGSGenerateES6",
          "when": "editorTextFocus"  
        },
        {
          "command": "extension.generateGetterSetter",
          "key": "ctrl+win+shift+x",
          "mac": "ctrl+f10",
          "when": "editorTextFocus"
        },
        {
          "command": "extension.generateGetterSetterForAll",
          "key": "ctrl+win+shift+z",
          "when": "editorTextFocus"
        },
        {
          "key": "ctrl+alt+h", 
          "command": "extension.SelectGSGenerate",
          "when": "editorTextFocus"  
        }
      ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "ts-morph": "^24.0.0",
    "typescript": "^5.6.3"
  }
}
